#friends.panel
  .notification

  .summary
    %h2 Friends
    .contacts(data-bind="foreach: friends")
      .row
        .icon.span2
          %img(data-bind="attr: {src: icon_uri}")
        .name.span3(data-bind="text: name")
        .jid.span3(data-bind="text: jid")
        .status.span2(data-bind="text: status")
        .action.span2
          %button.btn.danger.delete Remove

 
